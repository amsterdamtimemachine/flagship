{
  "name": "@atm/preprocessor",
  "version": "1.0.0",
  "description": "Data preprocessor for Amsterdam Time Machine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "generate": "../../scripts/sync-env.sh && bun --env-file=.env src/main.ts",
    "generate:docker": "bun src/main.ts",
    "test": "../../scripts/sync-env.sh && bun --env-file=.env test src/tests/",
    "test:binary": "../../scripts/sync-env.sh && bun --env-file=.env test src/tests/binaryDataValidation.test.ts",
    "test:generation": "../../scripts/sync-env.sh && bun --env-file=.env test src/tests/dataGeneration.test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@atm/shared": "workspace:*",
    "@msgpack/msgpack": "^3.1.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "bun-types": "latest",
    "eslint": "^9.29.0",
    "typescript": "^5.8.3"
  }
}
